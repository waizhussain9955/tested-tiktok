{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "api/index.py"
        },
        {
            "source": "/static/(.*)",
            "destination": "public/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "public/$1"
        }
    ]
}